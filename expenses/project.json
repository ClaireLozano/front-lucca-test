{
	"name": "expenses",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "expenses/src",
	"prefix": "exp",
	"tags": [],
	"projectType": "library",
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "expenses/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["expenses/**/*.ts", "expenses/**/*.html"]
			}
		}
	}
}
