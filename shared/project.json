{
	"name": "shared",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "shared/src",
	"prefix": "shared",
	"tags": [],
	"projectType": "library",
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "shared/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["shared/**/*.ts", "shared/**/*.html"]
			}
		},
		"storybook": {
			"executor": "@storybook/angular:start-storybook",
			"options": {
				"port": 4400,
				"configDir": "shared/.storybook",
				"browserTarget": "shared:build-storybook",
				"compodoc": false
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"build-storybook": {
			"executor": "@storybook/angular:build-storybook",
			"outputs": ["{options.outputDir}"],
			"options": {
				"outputDir": "dist/storybook/shared",
				"configDir": "shared/.storybook",
				"browserTarget": "shared:build-storybook",
				"compodoc": false
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"test-storybook": {
			"executor": "nx:run-commands",
			"options": {
				"command": "test-storybook -c shared/.storybook --url=http://localhost:4400"
			}
		},
		"static-storybook": {
			"executor": "@nx/web:file-server",
			"options": {
				"buildTarget": "shared:build-storybook",
				"staticFilePath": "dist/storybook/shared"
			},
			"configurations": {
				"ci": {
					"buildTarget": "shared:build-storybook:ci"
				}
			}
		}
	}
}
